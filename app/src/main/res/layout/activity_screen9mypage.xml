<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".screen9mypage">


    <Button
        android:id="@+id/button16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="280dp"
        android:layout_marginTop="10dp"
        android:text="뒤로"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/layLine"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:layout_marginTop="100dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="300dp"
            android:layout_marginTop="100dp"
            android:orientation="vertical"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/textmy1"
                android:layout_width="70dp"
                android:layout_height="50dp"
                android:layout_marginEnd="8dp"
                android:gravity="center"
                android:ems="10"
                android:text="아이디"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/textmy2"
                android:layout_width="70dp"
                android:layout_height="50dp"
                android:layout_marginTop="12dp"
                android:gravity="center"
                android:ems="10"
                android:text="비밀번호"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/textmy3"
                android:layout_width="70dp"
                android:layout_height="50dp"
                android:layout_marginTop="12dp"
                android:gravity="center"
                android:ems="10"
                android:text="전화번호"
                android:textSize="15sp" />


            <TextView
                android:id="@+id/textmy4"
                android:layout_width="70dp"
                android:layout_height="50dp"
                android:layout_marginTop="12dp"
                android:gravity="center"
                android:ems="10"
                android:text="생일"
                android:textSize="15sp" />

        </LinearLayout>


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="100dp"
            android:orientation="vertical"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <EditText
                android:id="@+id/textmy5"
                android:layout_width="200dp"
                android:layout_height="50dp"
                android:layout_marginEnd="12dp"
                android:ems="10"
                android:text="아이디"
                android:gravity="center"
                android:textAlignment="center"
                android:textSize="15sp" />

            <EditText
                android:id="@+id/textmy6"
                android:layout_width="200dp"
                android:layout_height="50dp"
                android:layout_marginTop="12dp"
                android:ems="10"
                android:text="비밀번호"
                android:gravity="center"
                android:textAlignment="center"
                android:textSize="15sp" />

            <EditText
                android:id="@+id/textmy7"
                android:layout_width="200dp"
                android:layout_height="50dp"
                android:layout_marginTop="12dp"
                android:ems="10"
                android:text="전화번호"
                android:gravity="center"
                android:textAlignment="center"
                android:textSize="15sp" />


            <EditText
                android:id="@+id/textmy8"
                android:layout_width="200dp"
                android:layout_height="50dp"
                android:layout_marginTop="12dp"
                android:ems="10"
                android:text="생일"
                android:gravity="center"
                android:textAlignment="center"
                android:textSize="15sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="100dp"
            android:orientation="vertical"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <Button
                android:id="@+id/btmy1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="8dp"
                android:ems="10"
                android:text=" "
                android:textSize="15sp" />

            <Button
                android:id="@+id/btmy2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="14dp"
                android:ems="10"
                android:text="바꾸기"
                android:textSize="15sp" />

            <Button
                android:id="@+id/btmy3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="14dp"
                android:ems="10"
                android:text="바꾸기"
                android:textSize="15sp" />


            <Button
                android:id="@+id/btmy4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="14dp"
                android:ems="10"
                android:text="바꾸기"
                android:textSize="15sp" />

        </LinearLayout>



    </LinearLayout>


    <Button
        android:id="@+id/button17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="확인"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/layLine" />









</androidx.constraintlayout.widget.ConstraintLayout>